
public class arrTest {
	public static void main(String[] arg) {
		byte[] bts = new byte[100];
		for(int i=0;i<100;i++) {
			bts[i] = (byte) i;
		}
		
		byte[] bt = new byte[20];
		byte[] bts1000 = new byte[1000];
		System.arraycopy(bts, 9, bts1000, 0, 20);
		for(int i=0;i<20;i++) {
			 System.out.println(bt[i]);
		}
	}
}
