
public class arrTest {
	public static void main(String[] arg) {
		byte[] bts = new byte[100];
		for(int i=0;i<100;i++) {
			bts[i] = (byte) i;
		}
		
		byte[] bt = new byte[20];
		System.arraycopy(bts, 7, bt, 0, bt.length);
		for(int i=0;i<20;i++) {
			 System.out.println(bt[i]);
		}
	}
}
