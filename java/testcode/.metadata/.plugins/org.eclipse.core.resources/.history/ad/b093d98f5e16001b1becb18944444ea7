package gateway.core.mapper;

import java.util.List;

import org.apache.ibatis.annotations.Param;
import org.apache.ibatis.annotations.Select;

import gateway.core.config.Function; 

public interface FunctionMapper {
	@Select("select * from function where state=1")
	public List<Function> getList();
	@Select("select * from function where deviceId = #{deviceId} and code = #{type} and devtype = #{type}")
	public List<Function> getAddrFun(
		@Param("deviceId") int deviceId,
		@Param("type") int type,
		@Param("code") int code); 
}
